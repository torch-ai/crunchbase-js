    "@rollup/plugin-json": "^4.0.0",
    "@types/jest": "^24.0.24",
    "dotenv": "^8.2.0",
    "jest": "^25.1.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.13",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^25.2.1",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "tslib": "^2.0.3"
  }
